{
  "name": "upyun-form",
  "version": "0.4.0",
  "description": "a pure front-end upyun form upload service, supports both native js and angular.js",
  "main": "src/upyun.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "serve": "serve .",
    "watch": "rewatch src/*.js -c 'npm run build'",
    "build": "uglifyjs src/upyun.js --mangle --compress -o dist/upyun.min.js --source-map dist/upyun.min.js.map --source-map-url upyun.min.js.map --source-map-root ../src/upyun.js",
    "example": "open examples/native.html",
    "dev": "npm run serve & npm run watch",
    "start": "npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/turingou/upyun.git"
  },
  "keywords": [
    "upyun",
    "form",
    "uploader",
    "upload",
    "angular",
    "angular.js"
  ],
